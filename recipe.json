{
	"name": "asyncdb",
	"compiler": "g++ -g -Wall -Wextra -Werror -std=c++11",
	"packaging": "executable",
	"modules": [],
	"dependencies":
		[
			{
				"package": "rocksdb"
			},
			{
				"package": "gtest",
				"scope": "test"
			},
			{
				"package": "gmock",
				"scope": "test"
			},
			{
				"package": "libcurl",
				"scope": "test"
			}
		],
	"plugins":
		[
			{
				"name": "version",
				"phase": "validate",
				"config":
					{
					}
			},
			{
				"name": "cppcheck",
				"phase": "validate",
				"config":
					{
						"options": "--enable=style"
					}
			},
			{
				"name": "gcovr",
				"phase": "verify",
				"config":
					{
						"options": "--fail-under-line 90"
					}
			}
		],
	"define": {}
}
